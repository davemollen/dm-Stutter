 @prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .

<https://github.com/davemollen/dm-Stutter>
	a lv2:Plugin , lv2:DelayPlugin , mod:DelayPlugin;
	lv2:project <http://lv2plug.in/ns/lv2> ;
	doap:name "dm-Stutter" ;
	doap:license "GPLv3" ;
	doap:maintainer [
		foaf:name "Dave Mollen" ;
		foaf:homepage <https://github.com/davemollen/dm-Stutter> ;
	] ;
	mod:brand "DM" ;
	mod:label "Stutter" ;
	rdfs:comment """
A stuttering effect written in Rust.
""" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "on" ;
		lv2:name "On" ;
		lv2:portProperty lv2:integer, lv2:toggled;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "auto" ;
		lv2:name "Auto" ;
		lv2:portProperty lv2:integer, lv2:toggled;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "trigger" ;
		lv2:name "Trigger" ;
		lv2:portProperty lv2:integer, lv2:toggled;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "pulse" ;
		lv2:name "Pulse" ;
		lv2:portProperty mod:tempoRelatedDynamicScalePoints ;
		lv2:default 500.0 ;
		lv2:minimum 10.0 ;
		lv2:maximum 2000.0 ;
		units:unit units:ms
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "duration" ;
		lv2:name "Duration" ;
		lv2:default 50.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 100.0 ;
		units:unit units:pc
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "chance" ;
		lv2:name "Chance" ;
		lv2:default 50.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 100.0 ;
		units:unit units:pc
	], [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 6 ;
		lv2:symbol "input" ;
		lv2:name "Input"
	], [
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 7 ;
		lv2:symbol "output" ;
		lv2:name "Output"
	] .
